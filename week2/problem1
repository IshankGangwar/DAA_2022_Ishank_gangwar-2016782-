//Find the no. of copies a element is present in the array and if element is not present print not present
#include<iostream>
#include<vector>
using namespace std;
int main(){
 vector <int> v1;
 cout<<"Enter the elements to be entered"<<endl;
 int n;
 cin>>n;
  for(int i=0;i<n;i++)
  {
      int a;
      cin>> a;
     v1.push_back(a);

  }
  int a;
  cin>>a;
  int count=0;
  for(int i=0;i<n;i++)
  {
      if(a==v1[i])
      count++;
  } 
  if(count==0)
  cout<<"the number"<< a<<" is not present "<<endl;
   else
   cout<<"the number "<< a<<" has"<<count<<"frequency";

return 0;
}
